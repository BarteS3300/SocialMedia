<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="159.0" prefWidth="313.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21"><!-- fx:controller="MAP.interfaces.GUIController" -->
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <BorderPane prefHeight="125.0" prefWidth="273.0">
         <right>
            <Button mnemonicParsing="false" onAction="#UpdateWindow" text="Update User" BorderPane.alignment="CENTER" />
         </right>
         <bottom>
            <HBox alignment="CENTER" prefHeight="63.0" prefWidth="273.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#FindOneWindow" text="Find One User" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" prefHeight="69.0" prefWidth="100.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#GetAllWindow" text="Get All Users" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </bottom>
         <left>
            <Button mnemonicParsing="false" onAction="#AddWindow" text="Add User" BorderPane.alignment="CENTER" />
         </left>
         <center>
            <Button mnemonicParsing="false" onAction="#DeleteWindow" text="Delete User" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</VBox>
